# Environment variables for Grower Slot SaaS

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/grower_slots
PGPORT=5432
PGUSER=user
PGPASSWORD=password
PGDATABASE=grower_slots
PGHOST=localhost

# Authentication
JWT_SECRET=your-jwt-secret-key-change-in-production

# Email (SendGrid)
SENDGRID_API_KEY=your-sendgrid-api-key

# Feature Flags (Frontend)
VITE_FEATURE_ADMIN_TEMPLATES=false
VITE_FEATURE_NEXT_AVAILABLE=false
VITE_FEATURE_DEBUG_LOGS=false

# Development
NODE_ENV=development